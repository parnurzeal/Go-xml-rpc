<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Go XML-RPC Library</title>
 <link href="http://sionide21.github.com/Go-xml-rpc/atom.xml" rel="self"/>
 <link href="http://sionide21.github.com/Go-xml-rpc/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://sionide21.github.com/Go-xml-rpc/</id>
 <author>
   <name>Dr Nic Williams</name>
   <email>sionide21williams@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://sionide21.github.com/Go-xml-rpc{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://sionide21.github.com/Go-xml-rpc{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>